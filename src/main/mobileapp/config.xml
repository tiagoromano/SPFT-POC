<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cronapp.apps.spftpoc" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SPFT-POC</name>
    <description>SPFT-POC</description>
    <author email="techne@techne.com.br" href="http://cronapp.io">Usu√°rio de Teste</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="res/icon.png" />
    <splash src="res/screen.png" />
    <engine name="android" spec="^7.0.0" />
    <plugin name="cordova-plugin-cronapp" spec="^2.1.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
